{
    "name": "dognewsreviewapp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "npx react-native start",
        "test": "npx jest",
        "lint": "npx eslint .",
        "pretty": "npx prettier --write '{app,lib,__tests__,__mocks__}/**/*.{js,jsx,ts,tsx}'",
        "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
        "bundle-ios": "npx react-native bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
        "fontawesome": "cp node_modules/react-native-vector-icons/Fonts/FontAwesome*.ttf android/app/src/main/assets/fonts/"
    },
    "dependencies": {
        "@types/lodash": "^4.14.149",
        "cd": "0.3.x",
        "lodash": "4.x.x",
        "prop-types": "15.x.x",
        "react": "^16.12.0",
        "react-native": "^0.61.5",
        "react-native-animatable": "^1.3.3",
        "react-native-app-auth": "4.x.x",
        "react-native-collapsible": "1.x",
        "react-native-gesture-handler": "^1.5.2",
        "react-native-loading-spinner-overlay": "1.x.x",
        "react-native-vector-icons": "6.x.x",
        "react-native-webview": "7.x.x",
        "react-navigation": "^3.13.0",
        "react-router-native": "^5.1.2",
        "react-zlib-js": "1.x.x"
    },
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/runtime": "^7.7.7",
        "@react-native-community/eslint-config": "0.0.5",
        "@types/jest": "^24.0.24",
        "@types/react": "^16.9.17",
        "@types/react-native": "^0.60.25",
        "@types/react-native-collapsible": "0.x.x",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "eslint": "^6.8.0",
        "eslint-plugin-jest": "^22.21.0",
        "eslint-plugin-react": "^7.17.0",
        "eslint-utils": "^1.4.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "metro-react-native-babel-preset": "0.54.x",
        "prettier": "1.x.x",
        "react-native-cli": "2.x.x",
        "react-test-renderer": "^16.12.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "preset": "react-native",
        "automock": false,
        "transformIgnorePatterns": [
            "node_modules/react-native"
        ],
        "testMatch": [
            "**/tests/*.js?(x)",
            "**/?(*.)(spec|test).js?(x)"
        ],
        "setupFiles": [
            "./setupJest.js"
        ]
    }
}
